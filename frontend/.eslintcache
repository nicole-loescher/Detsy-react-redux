uthenticate-me/frontend/src/components/ProductList/index.js",["107"],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/ProductDetail/index.js",["108","109"],"import { useEffect, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { EditProduct } from '../EditProduct'\nimport { deleteProduct } from '../../store/product'\nimport { addToCart } from '../../store/cart'\nimport './ProductDetail.css';\nimport { ReviewForm } from '../ReviewForm';\nimport { Rating } from '../Rating';\nimport { Redirect, useHistory } from 'react-router-dom';\n\n\nexport function ProductDetail({ product }) {\n    const user = useSelector(state => state.session.user);\n    const dispatch = useDispatch();\n    const [hideForm, setHideForm ]= useState(true);\n    const [hideReview, setHideReview] = useState(true);\n    const [hideReviewForm, setHideReviewForm]= useState(true);\n    const history = useHistory();\n\n    const signIn = (e) =>{\n        e.preventDefault()\n        history.push('/login')\n    }\n    \n        let displayReview;\n        if (product.Reviews[0]) {\n            displayReview = (\n                <p className='high-review'>'{product.Reviews[0].comments}' </p>\n            )\n        }\n        else if(!product.Reviews[0]){\n            displayReview = (\n                <p className='high-review'> No reviews yet..</p>\n            )\n        }\n            let content;\n            let innerContent;\n           \n        if (!user || user.id !== product.user_id){\n            if (!user) {\n                innerContent = (\n                    <button className='product__add' onClick={signIn}>Sign in to add to cart</button>                  \n                )\n            }\n            else {\n                innerContent = (\n                    <button className='product__add' onClick={() => dispatch(addToCart(product.id))}>Add to cart</button>\n                )\n            }\n            content = (\n                <div>\n                    {innerContent}\n                    {displayReview}\n                    <button className='product__add' onClick={()=> setHideReview(false)}>see more reviews</button>\n                </div>\n            )\n            let innerReviewContent;\n            let reviewContent;\n            if(!hideReview){\n                if(!product.Reviews[0]){\n                    reviewContent = (\n                        <h2>No reviews yet...</h2>\n                        )\n                    }\n                else if(product.Reviews){\n                    reviewContent = (\n                           <div>\n                               {product.Reviews.map(review => {\n                                   return (\n                                        <span className='review-list-item' key={review.id}>\n                                            <Rating review={review} />\n                                        </span>\n                                    ) \n                                })}\n                           </div>\n                    )\n                }\n                else {\n                    reviewContent = (\n                         <h2>loading reviews</h2>\n                    )\n                }\n                if(user){\n                 innerReviewContent = (\n                     <button className='review-buttons' onClick={()=> setHideReviewForm(false)}>Add review</button>\n                     )   \n                    }\n                else if(!user){\n                    innerReviewContent = (\n                        <button className='review-buttons' onClick={signIn}>Add review</button>\n                    )\n                }\n                content = (\n                    <div>\n                        {innerContent}\n                        {innerReviewContent}\n                        <button className='review-buttons' onClick={()=> setHideReview(true)}>Hide reviews</button>\n                        {reviewContent}\n                    </div> \n                )\n            }\n            if(!hideReviewForm){\n                content = (\n                        <ReviewForm product={product} hide={()=> setHideReviewForm(true)} hideForm={()=> setHideReview(true)} />  \n                )\n            }\n        }\n        else if(user.id === product.user_id){\n            content = (\n                <button className='product-edit__button' onClick={(e)=> setHideForm(false)}>\n                    Edit\n                </button>\n            )\n            if(!hideForm){\n                content=(\n                        <EditProduct product={product} hideForm={() => setHideForm(true)} deleteProduct={() =>{\n                        dispatch(deleteProduct(product.id))}\n                        }/>\n                )\n            }\n        }\n    \n    return (\n        <div className='product' key={`${product.id}.${product.name}`}>\n            <img alt='product' className='product__img' src={product.imgPath} />\n            <h2 className='product__title'>{product.name}</h2>\n            <p className='product__details'>{product.description}</p>\n            <h3 className='product__price'>${product.price}</h3>\n            {content}\n        </div>\n    )\n}","/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/Home/index.js",[],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/store/product.js",["110"],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/ProductForm/index.js",[],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/EditProduct/index.js",[],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/ReadProduct/index.js",[],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/ShoppingCart/index.js",[],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/store/cart.js",[],"/Users/nicolelolo/Desktop/authenticate-me/frontend/src/components/ShoppingCart/CartItem.js",[],"/Use